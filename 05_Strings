const name = "Mary";
const repoCount = 20;

console.log(name + repoCount + "Value");

console.log(`Hello my name is ${name} and my repo count is ${repoCount}`);  //String Interpolation

const gameName = new String('Hello Everyone');

console.log(gameName[0]); //Access of Key Value Pair
console.log(gameName.__proto__);

console.log(gameName.length);
console.log(gameName.toUpperCase());
console.log(gameName.charAt(6));
//console.log(gameName.anchor());
console.log(gameName.at(6));
//console.log(gameName.big());----deprecated
//console.log(gameName.blink());----deprecated
//console.log(gameName.bold());----deprecated
console.log(gameName.charCodeAt());//The charCodeAt() method returns the Unicode of the character at a specified index (position) in a string.The index of the first character is 0, the second is 1, ....
console.log(gameName.codePointAt());//The codePointAt() method returns the Unicode value at an index (position) in a string.
console.log(gameName.concat(" Present Here"));
//console.log(gameName.constructor);
console.log(gameName.endsWith('E'));
//console.log(gameName.fixed());----deprecated
//console.log(gameName.fontcolor());-----deprecated
//console.log(gameName.fontsize());--------deprecated
console.log(gameName.includes('Everyone'));
console.log(gameName.indexOf('E'));
console.log(gameName.isWellFormed());//The isWellFormed() method returns true if a string is well formed.Otherwise it returns false.
//console.log(gameName.italics());-----deprecated
console.log(gameName.lastIndexOf("Everyone"));
//console.log(gameName.link());-----deprecated
//console.log(gameName.localeCompare());
console.log(gameName.match("ello"));//The match() method matches a string against a regular expression ** The match() method returns an array with the matches.
//console.log(gameName.matchAll());
//console.log(gameName.normalize());
console.log(gameName.padEnd());




